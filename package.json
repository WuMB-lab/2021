    "fetch-ie8": "^1.5.0",
    "fs": "^0.0.1-security",
    "jsencrypt": "^3.0.0-rc.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isarray": "^4.0.0",
    "lodash.isfunction": "^3.0.9",
    "sinobest-functions": "^0.3.6",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
