{
  "name": "sinogear-frontend-starter-vue",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "node before.dev.js && cross-env ESLINT=none BROWSER=none RUN_ENV=dev PORT=8001 HOST=0.0.0.0 vue-cli-service serve",
